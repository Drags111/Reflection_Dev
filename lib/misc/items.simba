{*******************************************************************************
Function R_ReturnItemIDs(ItemName: String): TIntegerArray;
By: N1ke, Cstrike
Description: Returns all items based on your request.
*******************************************************************************}
Function R_ReturnItemIDs(ItemName: String): TIntegerArray;
var
  TSA : TStringArray;
begin
  R_ReturnItem(ItemName, Result, TSA);
end;

{*******************************************************************************
Function R_ReturnItemNames(ID: Integer): TStringArray;
By: N1ke, Cstrike
Description: Returns all items based on your request.
*******************************************************************************}
Function R_ReturnItemNames(ID: Integer): TStringArray;
var
  TIA: TIntegerArray;
begin
  R_ReturnItem(ID, TIA, Result);
end;

{*******************************************************************************
Function R_ReturnItemID(ItemName: String): Integer;
By: N1ke, Cstrike
Description: Returns first item based on your request.
*******************************************************************************}
Function R_ReturnItemID(ItemName: String): Integer;
var
  TIA : TIntegerArray;
  TSA : TStringArray;
begin
  Result := -1;
  R_ReturnItem (ItemName, TIA, TSA);
  if (GetArrayLength(TIA) > 0) then
    Result := TIA[0];
end;

{*******************************************************************************
Function R_ReturnItemName(ID: Integer): String;
By: N1ke, Cstrike
Description: Returns first item based on your request.
*******************************************************************************}
Function R_ReturnItemName(ID: Integer): String;
var
  TIA : TIntegerArray;
  TSA : TStringArray;
begin
  Result := '';
  R_ReturnItem (ID, TIA, TSA);
  if (GetArrayLength(TIA) > 0) then
    Result := TSA[0];
end;
