var
  RCerterVars_NPC: TNPC;

function RCerter_InRandom: Boolean;
var
  Obj: TRSObject;
begin
  Result := R_FindNPCMulti(['Giles', 'Miles', 'Niles'], RCerterVars_NPC);
  if Result then
  begin
    Obj := R_GetObjectByID(11368, OBJ_INTERACTABLE, 15);
    Result := (Obj <> NULL_RSOBJECT);
  end;
end;

function RCerter_Solve: Boolean;
begin
  Result := False;
end;
